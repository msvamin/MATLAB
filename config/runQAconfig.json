{
	"Version": "V5.3",	
	"PipeMode": "auto",	
	"RunList": [""],
	"Paths":{"load": {"root": "/Data", "raw": "hdf5/reads", "calibration": "hdf5/calibration",
						"metadata": "metadata"},
			 "save": {"root": "/NAS", "result": "result", "screening": "screening", "metadata": "MetaData",
						"chip": "Chip", "sensors": "SingleSensors", "bubble": "Bubble"},
			 "log": "PipelineReports/PipeLogs"},
	"Initialization": 1,
	"Plotting": 1,
	"ClearBigData": 1,
	"Report": 1,
	"runQAtype": "Regular",
	"BubbleMovie": 1,
	"RestAPI": {"logging": "/NAS/PipelineReports/RESTAPIlog/RESTAPIlog.csv",
				"runList": "http://genapsys-services.appspot.com/listOfRunsToBeProcessed?stateInclude=%s&keywork=&intervalDays=%s&stateExclude=7",
				"runInf": "http://genapsys-services.appspot.com/getExperimentInfoByIdAndState?experimentId=%s &state=start",	
				"runParams": "http://genapsys-services.appspot.com/getRunParamsByExperimentId?experimentId=%s",
				"Operator": "http://genapsys-services.appspot.com/getUserInfoByExperimentID?experimentId=%s",
				"CheckChip": "http://genapsys-services.appspot.com/getExperimentChipCheck?experimentId=%s",
				"runState": "http://genapsys-data.appspot.com/rest/experimentState",
				"runSpecs": "http://genapsys-services.appspot.com/experimentSpecTests"},
	"DACread": { "filename": "twoStaged_bathtub.h5", "path": "hdf5/calibration" },	
	"ScriptLog": ".scriptHandler.log",	
	"instrument": [ { "name": "flowrate", "filename": "flowrate.csv", "path": "metadata", 
			 "cols": { "timestamp": "TimeStamp", "data": ["FlowSensor"] } },
			{"name": "flowrate2", "filename": "flowrate2.csv", "path": "metadata",
			 "cols": { "timestamp": "TimeStamp", "data": ["FlowSensor2"] } },
			{ "name": "chipTemperatures", "filename": "chipTemperatures.csv", "path": "metadata", 
			 "cols": { "timestamp": "TimeStamp", 
			 "data": ["targetChipTemperature","chipTemperatureIPTAT", "chipTemperatureRTDU",
				  "chipTemperatureRTDM", "chipTemperatureRTDL", "tempRise" ] } },
			{ "name": "chipVoltages", "filename": "chipVoltages.csv", "path": "metadata", 
			 "cols": { "timestamp": "TimeStamp", 
			 "data": ["IPTATVoltage","RTDUVoltage", "RTDMVoltage", "RTDLVoltage", "ADCVref",
				  "AFEVref", "RFVHi", "RFVlo", "PPOLY", "HR", "VhiNB", "VloNB" ] } },
			{ "name": "systemPressures", "filename": "systemPressures.csv", "path": "metadata", 
			 "cols": { "timestamp": "TimeStamp", 
			 "data": ["pressureSensor1", "pressureSensor2" ] } },
			{ "name": "systemVoltages", "filename": "systemVoltages.csv", "path": "metadata", 
			 "cols": { "timestamp": "TimeStamp", 
			 "data": ["DVDD12", "DVDD33", "AVDD33", "HISENS", "DVDD12_LOW", "DVDD33_LOW", 
				  "AVDD33_LOW" ] } },
			{ "name": "systemTemperatures", "filename": "systemTemperatures.csv", "path": "metadata", 
			 "cols": { "timestamp": "TimeStamp", 
			 "data": ["TempSensor1", "TempSensor2", "TempSensor3", "TempSensor4", "TempSensor5", "TempSensor6", "PeltTemp" ] } } ],
	"SensorReads": { "filename": "ParserV6.h5", "path": "hdf5/reads", "numRandSen": 1000 }
}
	
